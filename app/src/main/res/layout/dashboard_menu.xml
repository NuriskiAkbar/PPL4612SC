<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="70dp"
        android:clickable="true"
        android:src="@drawable/carticon"
        android:translationZ="99dp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        tools:visibility="visible" />

    <View
        android:id="@+id/divider3"
        android:layout_width="3dp"
        android:layout_height="40dp"
        android:layout_marginTop="4dp"
        android:translationZ="99dp"
        android:layout_marginEnd="52dp"
        android:background="@color/ungupalingmudabanget"
        android:clickable="true"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="@+id/imageView24"
        app:layout_constraintTop_toTopOf="@+id/imageView24"
        tools:visibility="visible" />

    <ImageView
        android:id="@+id/imageView25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="12dp"
        android:translationZ="99dp"
        android:clickable="true"
        android:src="@drawable/baseline_fastfood_white_18dp"
        android:visibility="visible"
        app:layout_constraintEnd_toStartOf="@+id/divider3"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imageView24"
        app:layout_constraintTop_toTopOf="@+id/imageView24"
        tools:visibility="visible" />

    <TextView
        android:id="@+id/tv_total_pesanan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="28dp"
        android:clickable="true"
        android:translationZ="99dp"
        android:textColor="@color/ungupalingmudabanget"
        android:textSize="18sp"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="@+id/imageView24"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/divider3"
        app:layout_constraintTop_toTopOf="@+id/imageView24"
        tools:text="0"
        tools:visibility="visible" />

    <FrameLayout
        android:id="@+id/layout_frame"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:gravity="center"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <ImageView
            android:id="@+id/iv_menu1"
            android:layout_width="40dp"
            android:layout_height="44dp"
            android:layout_weight="1"
            android:padding="10dp"
            app:srcCompat="@drawable/dashboard_selected" />

        <ImageView
            android:id="@+id/iv_menu2"
            android:layout_width="40dp"
            android:layout_height="44dp"
            android:layout_weight="1"
            android:padding="10dp"
            app:srcCompat="@drawable/food" />

        <ImageView
            android:id="@+id/iv_menu3"
            android:layout_width="40dp"
            android:layout_height="44dp"
            android:layout_weight="1"
            android:padding="10dp"
            app:srcCompat="@drawable/minuman" />
    </LinearLayout>



</androidx.constraintlayout.widget.ConstraintLayout>